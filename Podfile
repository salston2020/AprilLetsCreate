  # Facebook
  pod 'FBSDKLoginKit'

  # Google Sign IN
  pod 'GoogleSignIn'

  pod 'MessageKit'
  pod 'JGProgressHUD'
  pod 'RealmSwift'
  pod 'SDWebImage'

end
